[decompiler]
# A comma-separated list of decompiler backends to use.
# Supported backends: ghidra, radare2
backends = ghidra, radare2

[ghidra]
# Path to the Ghidra installation directory (e.g., /opt/ghidra_10.1.5_PUBLIC)
path = /path/to/ghidra

[radare2]
# Path to the radare2 executable. If not found, an auto-install will be attempted.
path = auto

[recompiler]
# Path to the C compiler to use for recompilation (e.g., /usr/bin/gcc).
compiler = gcc
# Flags to pass to the compiler.
flags = -O2 -w
# Password for the secure archive.
archive_password = infected